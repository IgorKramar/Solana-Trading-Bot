# Скопируйте этот файл в .env и заполните значения

# Solana
SOLANA_CLUSTER_URL=https://api.devnet.solana.com
SOLANA_WALLET_PATH=~/.config/solana/id.json
PROGRAM_ID=YOUR_PROGRAM_ID

# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Web App
REACT_APP_CLUSTER=devnet
REACT_APP_RPC_ENDPOINT=https://api.devnet.solana.com
REACT_APP_PROGRAM_ID=${PROGRAM_ID}

# Trading Bot
BOT_PRIVATE_KEY=your_bot_private_key
PYTH_ENDPOINT=https://api.pyth.network
JITO_AUTH_KEYPAIR=path/to/jito/keypair.json
JITO_BLOCK_ENGINE_URL=https://block-engine.jito.wtf

# Database (если потребуется)
DATABASE_URL=postgresql://user:password@localhost:5432/trading_bot

# API Keys
SERUM_DEX_PROGRAM_ID=9xQeWvG816bUx9EPjHmaT23yvVM2ZWbrrpZb9PusVFin

# Monitoring
SENTRY_DSN=your_sentry_dsn
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# Feature Flags
ENABLE_TELEGRAM_BOT=true
ENABLE_WEB_UI=true
ENABLE_MONITORING=true
MAX_CONCURRENT_TRADES=5
TRADE_SIZE_LIMIT=1000 